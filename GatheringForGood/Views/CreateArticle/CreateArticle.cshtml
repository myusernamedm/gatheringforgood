                        <a asp-for="TandC" class="link-style" id="CreateArticleTandCHeading" asp-controller="TermsAndConditions" asp-action="TermsAndConditions" asp-route-culture="@CultureInfo.CurrentCulture.Name">@_loc[Model.TandCHeading]</a>
                        <br />
                        <input asp-for="TandC" type="checkbox" id="AcceptTandCCheckbox" />
                        <span asp-validation-for="TandC" class="font-danger" id="CreateArticleTandCHeadingValidation"></span>
                    </div>
                    <br />
                    <button type="submit" id="CreateArticleSubmitButton" formaction="SaveUserArticle" class="btn btn-success" onclick="gtag('event','click',{'event_category':'Click','event_label':'CreateArticlePage-Button-CreateArticleSubmit'});">@_loc[Model.Submit]</button>
                    <button type="submit" id="CreateArticlePreviewButton" formaction="PreviewArticle" formtarget="_blank" class="btn btn-outline-success" onclick="gtag('event','click',{'event_category':'Click','event_label':'CreateArticlePage-Button-Preview'});">@_loc[Model.PreviewButton]</button>
                </form>
            </div>
        </div>
    </div>
