@model CreateArticleViewModel
@using Microsoft.AspNetCore.Identity
@using GatheringForGood.Data
@using System.Globalization
@inject SignInManager<ApplicationUser> SignInManager
@inject UserManager<ApplicationUser> UserManager
@{
    ViewData["Title"] = _loc[Model.PageTabTitle];
}
@section meta_head_tags{
    <meta name='description' content="GatheringForGood Articles - Bringing people together to help save the environment and reduce the impact of climate change. Create your first article today!" />
    <meta property='og:locale' content="en_US" />
    <meta property="og:type" content="website" />
    <meta property='og:title' content="GatheringForGood Articles - The place where ideas and expertise come together to solve the world's problems. Create your first article today!" />
    <meta property='og:description' content="GatheringForGood Articles - Bringing people together to help save the environment and reduce the impact of climate change. Create your first article today!" />
    <meta property='og:url' content="https://gatheringforgood.com/en/Articles/Articles" />
    <meta property='og:site_name' content="gatheringforgood" />
    <meta property='og:image' content="http://www.gatheringforgood.com/images/shareimagearticles.jpg" />
    <meta property='og:image:width' content="1200" />
    <meta property='og:image:height' content="600" />
    <meta property='og:image:alt' content="gatheringforgood sharing image" />
    <meta property="og:author" content="gatheringforgood" />
    <link rel="preload" as="image" href="@Model.GetEarthHandsReducedImageUrl" imagesrcset="@Model.GetEarthHandsReduced400ImageUrl 2.5x, @Model.GetEarthHandsReduced800ImageUrl 2x, @Model.GetEarthHandsReduced1200ImageUrl 1x, @Model.GetEarthHandsReduced1600ImageUrl 0.5x" sizes="100vw">
}
@section header_image{
    <div class="bg-img-non-home" id="CreateArticlesPageBanner">
        <nav class="navbar navbar-expand-lg navbar-light" id="main-navbar-create-articles-page">
            <div class="container">
                <a class="navbar-brand mt-20" onclick="changeactive('Home'); gtag('event','click',{'event_category':'Click','event_label':'TheWorldIsChokingPage-GoToHomeLogo'});" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/images/circle_logo.webp" id="_ArticlesPageLogoImg" width="40" height="40" style="margin:0px 20px;" alt="Logo Image">
                </a>
            </div>
        </nav>
}
@section header_content{
    <div class="container-title">
        <div class="block-title">
            <br>
            <a type="button" class="btn btn-success button-title" id="CreateArticleBackBtn" asp-area="" asp-controller="Articles" asp-action="Articles" asp-route-culture="@CultureInfo.CurrentCulture.Name">@_loc[Model.BackButton]</a>
            <h1 class="block-subtitle-text" id="CreateArticlesPageSubtitle" localize-content>@_loc[Model.Subtitle]</h1>
            <h2 class="block-title-text-non-home" id="CreateArticlesPageTitle" localize-content>@_loc[Model.Title]</h2>
        </div>
    </div>
}
</div>
<body class="body-bg-img">
    <div class="modal fade" id="articlesPageComingSoonModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content comingsoon-bg-img">
                <div class="modal-header">
                    <h3 class="modal-title" id="ComingSoonModalTitle">Modal Error!1</h3>
                    <button type="button" id="ComingSoonModalHeaderCloseButton" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="col justify-content-center align-items-center">
                    <h4 class="modal-body" id="articlesPageLearnMoreModalPara">Modal Error!2</h4>
                </div>
                <br />
                <div class="form-group thumb-block">
                    <form action="@Url.Action("SaveUserEntry","Articles")" method="post">
                        <label class="modal-body1 col-form-label" id="articlesPageComingSoonModalFormLabel">Modal Error!3</label>
                        <br />
                        <textarea class="form-control" name="newsfeedUserEntry" id="newsfeedUserEntry" type="text"></textarea>
                        <div class="modal-footer">
                            <button type="submit" id="ComingSoonModalFooterSubmitButton" class="btn btn-success" data-dismiss="articlesPageComingSoonModal" onclick="gtag('event','click',{'event_category':'Click','event_label':'Articles-Button-ComingSoonModalSubmit'});">@_loc[Model.Submit]</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="container">
        <div class="row">
            <div class="container-body">
                <partial name="_TreePurchasePartial" />
            </div>
        </div>
    </div>
    <hr />
    <br />
    <div class="container justify-content-center align-items-center">
        <div class="row justify-content-center align-items-center">
            <div class="col-lg-8 col-xl-8 justify-content-center align-items-center">
                <form asp-action="" enctype="multipart/form-data" method="post" id="CreateArticleForm" name="CreateArticleForm">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <div class="form-group hide justify-content-center align-items-center" style="margin: 10px" id="SelectPurchaseOption">
                        <input class="hide" asp-for="PurchaseType" id="TreePurchaseType" value="1">
                        <partial name="_TreePurchaseOptionsPartial" />
                    </div>
                    <div class="form-group">
                        <div class="align-to-top justify-content-center">
                            <div class="container-body">
                                <div class="row justify-content-center align-items-center">
                                    <div class="col">
                                        <p class="font-heading" id="CreateArticleYourArticleCardHeading">@_loc[Model.YourArticleCard]</p>
                                    </div>
                                </div>
                            </div>
                            <div class="container-body">
                                <div class="row justify-content-center align-items-center">
                                    <div class="col">
                                        <a class="link-style">
                                            <div class="card h-30 border-success m-2" style="max-width: 18rem; min-width: 18rem">
                                                <div class="embed-responsive embed-responsive-16by9" id="CardDynamicImageDiv">
                                                    <img class="card-img-top embed-responsive-item" id="CardDynamicImage" src="@Model.CardTitleImage" onerror="this.src='@Model.CardTitleImage'" onclick="gtag('event','click',{'event_category':'Click','event_label':'CreateArticleCard-Link-Image'});" alt="Article Card Title Image">
                                                </div>
                                                <div class="embed-responsive embed-responsive-16by9 hide" id="CardDynamicVideoDiv">
                                                    <iframe class="embed-responsive-item" src="" id="CardDynamicVideo" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen onclick="gtag('event','click',{'event_category':'Click','event_label':'CreateArticleCard-Link-Video'});"></iframe>
                                                </div>
                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col hide" id="CreateArticleFeaturedCardIcon">
                                                            <img href="javascript:void(0)" class="link-style d-none d-sm-none d-md-block d-lg-block d-xl-block" id="CreateArticleFeaturedCardIconImage" width="120" height="18" style="margin-bottom: 6px" src="@Model.FeaturedArticleUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.FeaturedTitle]" data-content="@_loc[Model.FeaturedText]" alt="Featured Article Icon">
                                                            <img href="javascript:void(0)" class="link-style d-block d-sm-block d-md-none d-lg-none d-xl-none" id="CreateArticleFeaturedCardIconImage" width="120" height="18" style="margin-bottom: 6px" src="@Model.FeaturedArticleUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.FeaturedTitle]" data-content="@_loc[Model.FeaturedText]" alt="Featured Article Icon">
                                                        </div>
                                                        <div class="col hide" id="CreateArticleTreePlantedCardIcon">
                                                            <img href="javascript:void(0)" class="link-style d-none d-sm-none d-md-block d-lg-block d-xl-block" id="CreateArticleTreePlantedCardIconImage" width="20" height="20" style="margin: 0px 0px 6px 10px" src="@Model.TreePlantedUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.TreePurchaseTitle]" data-content="@_loc[Model.TreePurchaseText]" alt="Tree Planted Icon">
                                                            <img href="javascript:void(0)" class="link-style d-block d-sm-block d-md-none d-lg-none d-xl-none" id="CreateArticleTreePlantedCardIconImage" width="20" height="20" style="margin: 0px 0px 6px 10px" src="@Model.TreePlantedUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.TreePurchaseTitle]" data-content="@_loc[Model.TreePurchaseText]" alt="Tree Planted Icon">
                                                        </div>
                                                    </div>
                                                    <h5 class="card-title" id="CreateArticleCardTitle"><localize>Card Title</localize></h5>
                                                    @if (Model.UserFullName == "" || Model.UserFullName == null)
                                                    {
                                                        <p class="font-style-content-small-black">@_loc[Model.Author]: </p><p class="font-danger" id="CreateArticleCardAuthorError">@_loc[Model.AddName]</p>
                                                    }
                                                    else
                                                    {
                                                        <p class="font-style-content-small-black" id="CreateArticleCardAuthor">@_loc[Model.Author]: @Model.UserFullName</p>
                                                    }
                                                    <p class="font-style-content-small" style="font-weight: normal;" id="CreateArticleCardSnippet"><localize>Card Description</localize></p>
                                                </div>
                                                <div class="card-footer" id="CardDynamicFooter">
                                                    <p class="text-muted font-small" id="CreateArticleCardDateTime" style="font-weight: normal;">@Model.PostedDateTime</p>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <input class="hide" asp-for="TreePurchaseStatus" id="TreePurchaseCheckedStatus">
                    </div>
                    <div class="form-group">
                        <label asp-for="ArticleTitle" class="font-heading" id="CreateArticleTitleHeading">@_loc[Model.ArticleTitleLabel]</label>
                        <input asp-for="ArticleTitle" class="form-control" id="CreateArticleTitleField" onkeypress="PopulateCard('Title')" onkeyup="PopulateCard('Title')" onchange="PopulateCard('Title')" onkeydown="PopulateCard('Title')">
                        <span asp-validation-for="ArticleTitle" class="font-danger" id="ArticleTitleValidation"></span>
                    </div>
                    <div class="form-group">
                        <div class="row">
                            <div class="col">
                                <label asp-for="FirstName" class="font-heading" id="FirstNameHeading">@_loc[Model.FirstNameHeading]</label>
                                <div class="info-padding" id="CreateArticleNameInfoIcon">
                                    <svg xmlns="http://www.w3.org/2000/svg" onclick="tooltip()" width="16" height="16" fill="green" class="bi bi-info-circle" viewBox="0 0 16 16" data-toggle="popover" data-trigger="hover" style="margin-right:10px" title="@_loc[Model.Author]" data-content="@_loc[Model.ToolTipInfo]">
                                        <path d="M8 15A7 7 0 1 1 8 1a7 7 0 0 1 0 14zm0 1A8 8 0 1 0 8 0a8 8 0 0 0 0 16z" />
                                        <path d="m8.93 6.588-2.29.287-.082.38.45.083c.294.07.352.176.288.469l-.738 3.468c-.194.897.105 1.319.808 1.319.545 0 1.178-.252 1.465-.598l.088-.416c-.2.176-.492.246-.686.246-.275 0-.375-.193-.304-.533L8.93 6.588zM9 4.5a1 1 0 1 1-2 0 1 1 0 0 1 2 0z" />
                                    </svg>
                                </div>
                                <input asp-for="FirstName" class="form-control" id="FirstNameField" onkeypress="PopulateCard('Name')" onkeyup="PopulateCard('Name')" onchange="PopulateCard('Name')" onkeydown="PopulateCard('Name')">
                            </div>
                            <div class="col">
                                <label asp-for="LastName" class="font-heading" id="LastLastNameHeading">@_loc[Model.LastNameHeading]</label>
                                <input asp-for="LastName" class="form-control" id="LastNameField" onkeypress="PopulateCard('Name')" onkeyup="PopulateCard('Name')" onchange="PopulateCard('Name')" onkeydown="PopulateCard('Name')">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="FeaturedCode" class="font-heading" id="FeaturedArticleCodeHeading">@_loc[Model.FeaturedHeading]</label>
                        <div>
                            <label class="font-info" id="FeaturedArticleCodeInfo">@_loc[Model.FeaturedInfo]</label>
                        </div>
                        <input asp-for="FeaturedCode" class="form-control" id="FeaturedArticleCodeField" onkeypress="showFeaturedIcon()" onkeyup="showFeaturedIcon()" onchange="showFeaturedIcon()" onkeydown="showFeaturedIcon()">
                    </div>
                    <div class="form-group" id="TitleImageButton">
                        <div class="container-body">
                            <div class="col">
                                <div class="row justify-content-center align-items-center">
                                    <label asp-for="TitleImageUrl" class="font-heading" id="ArticleTitleImageHeading">@_loc[Model.TitleImageLabel]</label>
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <label asp-for="TitleImageUrl" class="font-small" id="ArticleRecommendedDimensionsHeading">@_loc[Model.RecommendedDimensionsLabel]</label>
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <label asp-for="TitleImageUrl" class="font-small" id="ArticleMaxFileSizeHeading">@_loc[Model.MaxFileSizeLabel]</label>
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <label asp-for="TitleImageUrl" class="font-small" id="ArticleFileTypesHeading">@_loc[Model.FileTypesLabel]</label>
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <input type="file" name="TitleImageUrl" class="btn btn-outline-success" id="TitleImageUrl" asp-for="TitleImageUrl" value="@_loc[Model.AddTitleImageButton]" onchange="preview('CardDynamicImage'); validationFieldPopulation('TitleValidation')" onclick="gtag('event','click',{'event_category':'Click','event_label':'CreateArticlePage-Button-UploadTitleImage'});" />
                                </div>
                                <div class="row justify-content-center align-items-center" id="ArticleTitleImageValidationDiv">
                                    <span asp-validation-for="TitleImageValidation" class="font-danger" id="ArticleTitleImageValidation"></span>
                                </div>
                                <div class="row">
                                    <p class="font-danger hide" id="ArticleTitleImageSizeValidation">@_loc[Model.ImageSizeError]</p>
                                </div>
                                <div class="row">
                                    <p class="font-danger hide" id="ArticleTitleImageTypeValidation">@_loc[Model.ImageTypeError]</p>
                                    <input asp-for="TitleImageValidation" autocomplete="off" class="form-control hide" id="TitleImageValidationField">
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group" id="TitleVideoUrl">
                        <div class="container-body">
                            <div class="col">
                                <div class="row justify-content-center align-items-center">
                                    <label asp-for="TitleVideoUrl" class="font-heading" id="ArticleTitleVideoHeading">@_loc[Model.TitleVideoLabel]</label>
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <input asp-for="TitleVideoUrl" class="form-control titleVideoDescriptionField" id="ArticleTitleVideoField" style="resize:none;" onkeypress="validateVideoLinkTitle()" onkeyup="validateVideoLinkTitle()" onchange="validateVideoLinkTitle()">
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <p class="font-danger hide" id="ArticleTitleVideoValidation">@_loc[Model.VideoLinkError]</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group" id="TitleImageCaption">
                        <div class="container-body">
                            <div class="col">
                                <div class="row justify-content-center align-items-center">
                                    <label asp-for="ImageTitleCaption" class="font-heading" id="ImageTitleCaptionLabel">@_loc[Model.ImageCaptionLabel]</label>
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <input asp-for="ImageTitleCaption" class="form-control" id="ImageTitleCaptionField">
                                </div>
                                <div class="row justify-content-center align-items-center">
                                    <span asp-validation-for="ImageTitleCaption" class="font-danger" id="ImageTitleCaptionValidation"></span>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label asp-for="Snippet" class="font-heading" id="ArticleSnippetHeading">@_loc[Model.ArticleSnippetLabel]</label>
                        <textarea asp-for="Snippet" class="form-control snippetDescriptionField" id="ArticleSnippetField" style="resize:none;" onkeypress="PopulateCard('Snippet')" onkeyup="PopulateCard('Snippet')" onchange="PopulateCard('Snippet')" onkeydown="PopulateCard('Snippet')"></textarea>
                        <span asp-validation-for="Snippet" class="font-danger" id="ArticleSnippetValidation"></span>
                    </div>
                    <div class="form-group" id="Para1Group">
                        <label asp-for="Para1Text" class="font-heading" id="Para1Heading">@_loc[Model.Para1Label]</label>
                        <textarea asp-for="Para1Text" class="form-control paraField" style="resize:none;" id="Para1Field"></textarea>
                        <span asp-validation-for="Para1Text" class="font-danger" id="Para1Validation"></span>
                    </div>
                    <div class="form-group" id="Para2Buttons">
                        <div class="container-body">
                            <div class="col">
                                <div class="row justify-content-center align-items-center">
                                    <input type="button" class="btn btn-outline-success" id="ShowAddParaButtonForPara2" style="margin: 10px 10px 0px 0px" value="@_loc[Model.AddParaButton]" onclick="AddParaOrImage('Para2'); gtag('event','click',{'event_category':'Click','event_label':'CreateArticlePage-Button-AddPara2'});" />
                                    <input type="button" class="btn btn-outline-success" id="ShowAddImageButtonForImage2" style="margin: 10px 0px 0px 0px" value="@_loc[Model.AddImageButton]" onclick="AddParaOrImage('Image2'); gtag('event','click',{'event_category':'Click','event_label':'CreateArticlePage-Button-AddImage2'});" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group hide" id="Para2Group">
                        <label asp-for="Para2Subheading" class="font-heading" id="Para2SubheadingLabel">@_loc[Model.SubheadingLabel]</label>
                        <input asp-for="Para2Subheading" class="form-control" id="Para2SubheadingField">
                        <span asp-validation-for="Para2Subheading" class="font-danger" id="Para2SubheadingValidation"></span>
                        <label asp-for="Para2Text" class="font-heading" id="Para2Heading">@_loc[Model.Para2Label]</label>
                        <textarea asp-for="Para2Text" class="form-control paraField" style="resize:none;" id="Para2Field"></textarea>
                        <span asp-validation-for="Para2Text" class="font-danger" id="Para2Validation"></span>
                    </div>
                    <div class="form-group hide" id="Image2Group">
                        <div class="container-body">
                            <div class="row justify-content-center align-items-center">
                                <div class="col">
                                    <div class="row justify-content-center align-items-center">
                                        <label asp-for="Image2Url" class="font-heading" id="ArticleImage2Heading">@_loc[Model.Add1stImageButton]</label>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <label asp-for="Image2Url" class="font-small" id="Image2UrlRecommendedDimensionsHeading">@_loc[Model.RecommendedDimensionsLabel]</label>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <label asp-for="Image2Url" class="font-small" id="Image2UrlMaxFileSizeHeading">@_loc[Model.MaxFileSizeLabel]</label>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <label asp-for="Image2Url" class="font-small" id="Image2UrlFileTypesHeading">@_loc[Model.FileTypesLabel]</label>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <input type="file" name="Image2Url" id="Image2Url" class="btn btn-outline-success" asp-for="Image2Url" value="@_loc[Model.Add1stImageButton]" onchange="preview('Image2UrlImage'); validationFieldPopulation('Image2Validation')" onclick="gtag('event','click',{'event_category':'Click','event_label':'CreateArticlePage-Button-UploadImage2'});" />
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <p class="font-danger hide" id="CreateArticleImage2SizeError">@_loc[Model.ImageSizeError]</p>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <p class="font-danger hide" id="CreateArticleImage2TypeError">@_loc[Model.ImageTypeError]</p>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <label asp-for="Video2Url" class="font-heading" id="ArticleVideo2Heading">@_loc[Model.ParaVideoLabel]</label>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <input asp-for="Video2Url" class="form-control paraVideoDescriptionField" id="ArticleVideo2Field" style="resize:none;" onkeypress="validateVideoLinkParas('Image2Url', 'CreateArticleImage2SizeError', 'CreateArticleImage2TypeError', 'Image2UrlImage', 'ArticleVideo2Field', 'ArticleVideo2Validation', 'Image2UrlImageDiv', 'Image2UrlVideoDiv', 'Image2UrlVideo', 'Video2ValidationField')" onkeyup="validateVideoLinkParas('Image2Url', 'CreateArticleImage2SizeError', 'CreateArticleImage2TypeError', 'Image2UrlImage', 'ArticleVideo2Field', 'ArticleVideo2Validation', 'Image2UrlImageDiv', 'Image2UrlVideoDiv', 'Image2UrlVideo', 'Video2ValidationField')" onchange="validateVideoLinkParas('Image2Url', 'CreateArticleImage2SizeError', 'CreateArticleImage2TypeError', 'Image2UrlImage', 'ArticleVideo2Field', 'ArticleVideo2Validation', 'Image2UrlImageDiv', 'Image2UrlVideoDiv', 'Image2UrlVideo', 'Video2ValidationField')">
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <p class="font-danger hide" id="ArticleVideo2Validation">@_loc[Model.VideoLinkError]</p>
                                        <input asp-for="Video2Validation" autocomplete="off" class="form-control hide" id="Video2ValidationField" value="Ok">
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <label asp-for="Image2Caption" class="font-heading" id="Image2CaptionLabel">@_loc[Model.ImageCaptionLabel]</label>
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <input asp-for="Image2Caption" class="form-control" id="Image2CaptionField">
                                    </div>
                                    <div class="row justify-content-center align-items-center">
                                        <span asp-validation-for="Image2Caption" class="font-danger" id="Image2CaptionValidation"></span>
                                    </div>
                                </div>
                                <div class="col">
                                    <div class="row justify-content-center align-items-center">
                                        <div class="card h-30 border-success m-2" style="max-width: 16rem; min-width: 16rem">
                                            <div class="embed-responsive embed-responsive-16by9" id="Image2UrlImageDiv">
                                                <img class="card-img-top embed-responsive-item" id="Image2UrlImage" src="@Model.CardImage" onerror="this.src='@Model.CardImage'" alt="Article Card Image2">
                                            </div>
                                            <div class="embed-responsive embed-responsive-16by9 hide" id="Image2UrlVideoDiv">
                                                <iframe class="embed-responsive-item" src="" id="Image2UrlVideo" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen onclick="gtag('event','click',{'event_category':'Click','event_label':'Image2UrlVideo-Link-Video'});"></iframe>
                                            </div>
                                            <!--<iframe src="" id="CardDynamicVideo" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen onclick="gtag('event','click',{'event_category':'Click','event_label':'Articles-Link-Video'});"></iframe>;-->
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="form-group hide" id="Para3Buttons">
                        <div class="container-body">
                            <div class="col">
                                <div class="row justify-content-center align-items-center">
                                    <button type="button" class="btn btn-default" aria-label="Left Align" style="margin: 10px 0px 0px 0px; color: white; background-color: forestgreen" id="createArticleRotateIconForPara3" onclick="rotateParaImage('ParaImage2')"><i class="fa fa-refresh"></i></button>
                                </div>
                                <div class="row justify-content-center align-items-center">
