            }
            <h1 class="block-subtitle-text" id="OffsetMyCarbonPageSubtitle">@_loc[Model.Subtitle]</h1>
            <h2 class="block-title-text-non-home" id="OffsetMyCarbonPageTitle">@_loc[Model.Title]</h2>
        </div>
    </div>
}
