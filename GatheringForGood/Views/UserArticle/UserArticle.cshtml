@model UserArticleViewModel
@using System.Globalization
@{
    ViewData["Title"] = _loc[Model.PageTabTitle];
}
@section meta_head_tags{
    <meta name='description' content="GatheringForGood - @Model.ArticleTitle" />
    <meta property='og:locale' content="en_US" />
    <meta property="og:type" content="website" />
    <meta property='og:title' content="GatheringForGood - @Model.ArticleTitle" />
    <meta property='og:description' content="GatheringForGood - @Model.Para1" />
    <meta property='og:url' content="@Model.ArticleId" />
    <meta property='og:site_name' content="gatheringforgood" />
    @if (Model.ArticleTitleImageUrl != null)
    {
        <meta property='og:image' content="@Html.DisplayFor(modelItem => Model.ArticleTitleImageUrl)" />
    }
    else
    {
        <meta property='og:image' content="http://www.gatheringforgood.com/images/shareimagearticles.jpg" />
    }
    <meta property='og:image:width' content="1200" />
    <meta property='og:image:height' content="600" />
    <meta property='og:image:alt' content="gatheringforgood article @Model.ArticleTitle" />
    <meta property="og:author" content="gatheringforgood">
    <link rel="preload" as="image" href="@Model.GetEarthHandsReducedImageUrl" imagesrcset="@Model.GetEarthHandsReduced400ImageUrl 2.5x, @Model.GetEarthHandsReduced800ImageUrl 2x, @Model.GetEarthHandsReduced1200ImageUrl 1x, @Model.GetEarthHandsReduced1600ImageUrl 0.5x" sizes="100vw">
}
@section header_image{
    <div class="bg-img-non-home" id="UserArticlesPageBanner">
        <nav class="navbar navbar-expand-lg navbar-light" id="main-navbar-userarticles-page">
            <div class="container">
                <a class="navbar-brand mt-20" onclick="changeactive('Home'); gtag('event','click',{'event_category':'Click','event_label':'UserArticle-GoToHomeLogo'});" asp-area="" asp-controller="Home" asp-action="Index">
                    <img src="~/images/circle_logo.webp" id="_UserArticlesPageLogoImg" width="40" height="40" style="margin:0px 20px;" alt="Logo Image">
                </a>
            </div>
        </nav>
}
@section header_content{
    <div class="container-title">
        <div class="block-title">
            <br>
            @if (Model.ArticleId.Substring(Model.ArticleId.LastIndexOf("/") + 1) != "Preview")
            {
                <a button type="button" class="btn btn-success button-title" id="UserArticleBackBtn" asp-area="" asp-controller="Articles" asp-action="Articles" asp-route-culture="@CultureInfo.CurrentCulture.Name">@_loc[Model.GoBack]</a>
            }
            <h1 class="block-subtitle-text" id="UserArticlesPageSubtitle" localize-content>@_loc[Model.Subtitle]</h1>
            <h2 class="block-title-text-non-home" id="UserArticlesPageTitle" localize-content>@_loc[Model.Title]</h2>
        </div>
    </div>
}
</div>
<body class="body-bg-img">
    <div class="modal fade" id="userArticlesPageComingSoonModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content comingsoon-bg-img">
                <div class="modal-header">
                    <h3 class="modal-title" id="ComingSoonModalTitle">Modal Error!1</h3>
                    <button type="button" id="ComingSoonModalHeaderCloseButton" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="col justify-content-center align-items-center">
                    <h4 class="modal-body" id="userArticlesPageLearnMoreModalPara">Modal Error!2</h4>
                </div>
                <br />
                <div class="form-group thumb-block">
                    <form action="@Url.Action("SaveUserEntry","Articles")" method="post">
                        <label class="modal-body1 col-form-label" id="userArticlesPageComingSoonModalFormLabel">Modal Error!3</label>
                        <br />
                        <textarea class="form-control" name="newsfeedUserEntry" id="newsfeedUserEntry" type="text"></textarea>
                        <div class="modal-footer">
                            <button type="submit" id="ComingSoonModalFooterSubmitButton" class="btn btn-success" data-dismiss="userArticlesPageComingSoonModal" onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticle-Button-ComingSoonModalSubmit'});">@_loc[Model.Submit]</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
    <br />
    <div class="container-body">
        @if (Model.ArticleId.Substring(Model.ArticleId.LastIndexOf("/") + 1) == "Preview")
        {
            <div class="row justify-content-center align-items-center">
                <div class="col justify-content-center align-items-center">
                    <img src="@Model.IconTwitter" id="UserArticlesPreviewTwitterShareIconImage" width="30" height="30" style=" margin: 0px 10px 40px 0px;" alt="Twitter Share Icon">
                </div>
                <div class="col justify-content-center align-items-center">
                    <img src="@Model.IconFacebook" id="UserArticlesPreviewFBShareIconImage" width="30" height="30" style=" margin: 0px 10px 40px 0px;" alt="Facebook Share Icon">
                </div>
                <div class="col justify-content-center align-items-center">
                    <img src="@Model.IconLinkedin" id="UserArticlesPreviewLinkedinShareIconImage" width="30" height="30" style=" margin: 0px 10px 40px 0px;" alt="Linkedin Share Icon">
                </div>
            </div>
        }
        else
        {
            <div class="row justify-content-center align-items-center">
                <div class="col justify-content-center align-items-center">
                    <a href="@_loc[Model.UserArticleShare]" data-size="small"><img src="@Model.IconTwitter" id="UserArticlespageTwitterShareIconImage" width="30" height="30" style=" margin: 0px 10px 40px 0px;" alt="Twitter Share Icon" onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticle-Button-TwitterShareIcon'});"></a>
                </div>
                <div class="col justify-content-center align-items-center">
                    <div data-href="" data-layout="button" data-size="small"><a target="_blank" href="https://www.facebook.com/sharer/sharer.php?u=https://www.gatheringforgood.com/en/UserArticle/UserArticle/@Model.uniqueArticleReference&amp;src=sdkpreparse" class="fb-xfbml-parse-ignore"><img src="@Model.IconFacebook" id="UserArticlespageFBShareIconImage" width="30" height="30" style=" margin: 0px 10px 40px 0px;" alt="Facebook Share Icon" onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticle-Button-FacebookShareIcon'});"></a></div>
                </div>
                <div class="col justify-content-center align-items-center">
                    <a href="https://www.linkedin.com/shareArticle?mini=true&url=https://www.gatheringforgood.com/en/UserArticle/UserArticle/@Model.uniqueArticleReference&title=Make A Difference&summary=gatheringforgood.com&source=gatheringforgood" onclick="window.open(this.href, 'mywin', 'left=20,top=20,width=400,height=500,toolbar=1,resizable=0'); return false;" class="fb-xfbml-parse-ignore"><img src="@Model.IconLinkedin" id="UserArticlespageLinkedinShareIconImage" width="30" height="30" style=" margin: 0px 10px 40px 0px;" alt="Linkedin Share Icon" onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticle-Button-LinkedinShareIcon'});"></a>
                </div>
            </div>
        }
    </div>
    <div class="container-body">
        @if (Model.ArticleId.Substring(Model.ArticleId.LastIndexOf("/") + 1) == "Preview")
        {
            <div class="row justify-content-center align-items-center">
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <img src="@Model.LikeIconUrl" id="UserArticlePageLikeIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Like Icon" />
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.LikeCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <img src="@Model.DislikeIconUrl" id="UserArticlePageDislikeIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Dislike Icon" />
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.DislikeCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <img src="@Model.SupportIconUrl" id="UserArticlePageSupportIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Support Icon" />
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.SupportCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <img src="@Model.QuestionableIconUrl" id="UserArticlePageQuestionableIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Questionable Icon" />
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.QuestionableCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <img src="@Model.ShockedIconUrl" id="UserArticlePageShockedIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Shocked Icon" />
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.ShockedCount</p>
                </div>
            </div>
        }
        else
        {
            <div class="row justify-content-center align-items-center">
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <a href="@Url.Action("UserReaction", "UserArticle", new { actionNumber = 1, path = Context.Request.Path   })" data-toggle="popover" data-trigger="hover" data-content="@_loc[Model.LikeTooltip]" id="UserArticlePageLikeIconLink"><img src="@Model.LikeIconUrl" id="UserArticlePageLikeIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Like Icon" /></a>
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.LikeCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <a href="@Url.Action("UserReaction", "UserArticle", new { actionNumber = 2, path = Context.Request.Path })" data-toggle="popover" data-trigger="hover" data-content="@_loc[Model.DislikeTooltip]" id="UserArticlePageDislikeIconLink"><img src="@Model.DislikeIconUrl" id="UserArticlePageDislikeIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Dislike Icon" /></a>
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.DislikeCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <a href="@Url.Action("UserReaction", "UserArticle", new { actionNumber = 3, path = Context.Request.Path })" data-toggle="popover" data-trigger="hover" data-content="@_loc[Model.SupportTooltip]" id="UserArticlePageSupportIconLink"><img src="@Model.SupportIconUrl" id="UserArticlePageSupportIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Support Icon" /></a>
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.SupportCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <a href="@Url.Action("UserReaction", "UserArticle", new { actionNumber = 4, path = Context.Request.Path })" data-toggle="popover" data-trigger="hover" data-content="@_loc[Model.QuestionableTooltip]" id="UserArticlePageQuestionableIconLink"><img src="@Model.QuestionableIconUrl" id="UserArticlePageQuestionableIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Questionable Icon" /></a>
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.QuestionableCount</p>
                </div>
                <div class="col justify-content-center align-items-center" style="text-align: center">
                    <a href="@Url.Action("UserReaction", "UserArticle", new { actionNumber = 5, path = Context.Request.Path })" data-toggle="popover" data-trigger="hover" data-content="@_loc[Model.ShockedTooltip]" id="UserArticlePageShockedIconLink"><img src="@Model.ShockedIconUrl" id="UserArticlePageShockedIconImage" width="20" height="20" style=" margin: 0px 0px 10px 0px;" alt="Shocked Icon" /></a>
                    <p class="font-info-green" id="LikeIconCount" style=" margin: 0px 0px 0px 0px;">@Model.ShockedCount</p>
                </div>
            </div>
        }
    </div>
    @for (int i = 0; i < 1; i++)
    {
        var user = Model.ArticleUserId;

        var userDetails = Model.Context.Users.Find(user);
        var authorFirstName = "Anonymous";
        var authorLastName = "";

        @if (Model.AuthorFirstNameRef != "none" && Model.AuthorLastNameRef != "none")
        {
            authorFirstName = Model.AuthorFirstNameRef;
            authorLastName = Model.AuthorLastNameRef;
        }
        else if (Model.AuthorFirstNameRef != "none")
        {
            authorFirstName = Model.AuthorFirstNameRef;
        }
        else if (userDetails != null)
        {
            if (userDetails.FirstName == "" || userDetails.FirstName == null || userDetails.LastName == "" || userDetails.LastName == null)
            {
                authorFirstName = "Anonymous";
                authorLastName = "";
            }
            else
            {
                authorFirstName = userDetails.FirstName.ToString();
                authorLastName = userDetails.LastName.ToString();
            }
        }

        <div class="container-body justify-content-center align-items-center">
            <div class="col justify-content-center align-items-center">
                <div class="row justify-content-center align-items-center">
                    <br />
                    <h2 class="font-style-content-head thumb-text-block" id="ArticleTitle">@Model.ArticleTitle</h2>
                </div>
                @if (Model.Featured)
                {
                    <div class="row justify-content-center align-items-center">
                        <img href="javascript:void(0)" class="link-style d-none d-sm-none d-md-block d-lg-block d-xl-block" id="UserArticleFeaturedImage" style="width:200px; height:auto;" src="@Model.FeaturedArticleUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.FeaturedTitle]" data-content="@_loc[Model.FeaturedText]" alt="Featured Article Icon">
                        <img href="javascript:void(0)" class="link-style d-block d-sm-block d-md-none d-lg-none d-xl-none" id="UserArticleFeaturedImage" style="width:200px; height:auto;" src="@Model.FeaturedArticleUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.FeaturedTitle]" data-content="@_loc[Model.FeaturedText]" alt="Featured Article Icon">
                    </div>
                    <br />
                }
                @if (Model.TreePlanted)
                {
                    <div class="row justify-content-center align-items-center">
                        <img href="javascript:void(0)" class="link-style d-none d-sm-none d-md-block d-lg-block d-xl-block" id="UserArticleTreePlantedImage" style="width:80px; height:auto;" src="@Model.TreePlantedUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.TreePurchaseTitle]" data-content="@_loc[Model.TreePurchaseText]" alt="Tree Planted Icon">
                        <img href="javascript:void(0)" class="link-style d-block d-sm-block d-md-none d-lg-none d-xl-none" id="UserArticleTreePlantedImage" style="width:80px; height:auto;" src="@Model.TreePlantedUrl" onclick="tooltip()" data-toggle="popover" data-trigger="hover" title="@_loc[Model.TreePurchaseTitle]" data-content="@_loc[Model.TreePurchaseText]" alt="Tree Planted Icon">
                    </div>
                    <br />
                }
                <div class="row justify-content-center align-items-center">
                    <h4 class="font-style-content-medium-black" id="ArticleAuthor">@authorFirstName @authorLastName - @Model.PostedDateTime UTC</h4>
                </div>
                <br />
            </div>
            <br />
        </div>
        <div class="container">
            @if (Model.ArticleTitleImageUrl != null)
            {
                <div class="row justify-content-center align-items-center">
                    @if (Model.ArticleId.Substring(Model.ArticleId.LastIndexOf("/") + 1) != "Preview")
                    {
                        <img class="img-fluid max-width: 120%; height: auto;" src="@Html.DisplayFor(modelItem => Model.ArticleTitleImageUrl)" id="_ArticleTitleImage" width="280" height="160" style="margin:0px 20px;" alt="Article Title Image">
                    }
                    else
                    {
                        <img class="img-fluid max-width: 120%; height: auto;" src="~/previewimages/@Model.ArticleTitleImageUrl" id="_ArticleTitleImage" width="280" height="160" style="margin:0px 20px;" alt="Article Title Image">
                    }
                </div>
            }
            else if (Model.ArticleTitleVideoUrl != null)
            {
                <div class="row justify-content-center align-items-center">
                    <div class="video-container">
                        <iframe src="@Html.DisplayFor(modelItem => Model.ArticleTitleVideoUrl)" id="Video" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticle-Link-Video'});"></iframe>
                    </div>
                </div>
            }
            @if (Model.ImageTitleCaption != null)
            {
                <div class="row justify-content-center align-items-center">
                    <div class="font-small thumb-text-block" id="_ArticleImageTitleCaption">@Model.ImageTitleCaption</div>
                </div>
            }
        </div>

        <div class="container-body">
            <div class="col-lg-8">
                <br />
                <!--Note: Para 1 is used on every article-->
                @if (Model.Para1 != null)
                {
                    <div class="row">
                        <div class="font-style-content-black d-none d-sm-none d-md-none d-lg-block d-xl-block" id="_ArticlePara1">@Model.Para1</div>
                        <div class="font-style-content-medium-black d-none d-sm-block d-md-block d-lg-none d-xl-none" id="_ArticlePara1">@Model.Para1</div>
                        <div class="font-style-content-small-black d-block d-sm-none d-md-none d-lg-none d-xl-none" id="_ArticlePara1">@Model.Para1</div>
                    </div>
                    <br />
                }
                else if (Model.Image1 != null && Model.Para1 == null)
                {
                    <div class="row justify-content-center align-items-center">
                        <img class="img-fluid max-width: 120%; height: auto;" src="@Html.DisplayFor(modelItem => Model.Image1)" id="_ArticleImage1" width="480" height="260" style="margin:0px 20px;" alt="Article Environment Image 1">
                    </div>
                }
                else if (Model.Video1 != null && Model.Para1 == null && Model.Image1 == null)
                {
                    <div class="row justify-content-center align-items-center">
                        <div class="video-container">
                            <iframe src="@Html.DisplayFor(modelItem => Model.Video1)" id="_ArticleVideo1" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticleVideo1-Embedded-Video'});"></iframe>
                        </div>
                    </div>
                }

                @if (Model.Para2Subheading != null)
                {
                    <div class="row justify-content-center align-items-center">
                        <div class="font-style-article-subhead thumb-text-block d-none d-sm-none d-md-none d-lg-block d-xl-block" id="_ArticlePara2Subheading">@Model.Para2Subheading</div>
                        <div class="font-style-article-subhead-medium thumb-text-block d-none d-sm-block d-md-block d-lg-none d-xl-none" id="_ArticlePara2Subheading">@Model.Para2Subheading</div>
                        <div class="font-style-article-subhead-small thumb-text-block d-block d-sm-none d-md-none d-lg-none d-xl-none" id="_ArticlePara2Subheading">@Model.Para2Subheading</div>
                    </div>
                }
                @if (Model.Para2 != null)
                {
                    <div class="row">
                        <div class="font-style-content-black d-none d-sm-none d-md-none d-lg-block d-xl-block" id="_ArticlePara2">@Model.Para2</div>
                        <div class="font-style-content-medium-black d-none d-sm-block d-md-block d-lg-none d-xl-none" id="_ArticlePara2">@Model.Para2</div>
                        <div class="font-style-content-small-black d-block d-sm-none d-md-none d-lg-none d-xl-none" id="_ArticlePara2">@Model.Para2</div>
                    </div>
                    <br />
                }
                else if (Model.Image2 != null && Model.Para2 == null)
                {
                    <div class="row justify-content-center align-items-center">
                        @if (Model.ArticleId.Substring(Model.ArticleId.LastIndexOf("/") + 1) != "Preview")
                        {
                            <img class="img-fluid max-width: 120%; height: auto;" src="@Html.DisplayFor(modelItem => Model.Image2)" id="_ArticleImage2" width="480" height="260" style="margin:0px 20px;" alt="Article Environment Image 2">
                        }
                        else
                        {
                            <img class="img-fluid max-width: 120%; height: auto;" src="~/previewimages/@Model.Image2" id="_ArticleImage2" width="480" height="260" style="margin:0px 20px;" alt="Article Title Image">
                        }
                    </div>
                    @if (Model.Image2Caption == null && Model.Para3Subheading == null && Model.Para3 == null)
                    {
                        <br />
                    }
                }
                else if (Model.Video2 != null && Model.Para2 == null && Model.Image2 == null)
                {
                    <div class="row justify-content-center align-items-center">
                        <div class="video-container">
                            <iframe src="@Html.DisplayFor(modelItem => Model.Video2)" id="_ArticleVideo2" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticleVideo2-Embedded-Video'});"></iframe>
                        </div>
                    </div>
                }
                @if (Model.Image2Caption != null)
                {
                    <div class="row justify-content-center align-items-center">
                        <div class="font-small thumb-text-block" id="_ArticleImage2Caption">@Model.Image2Caption</div>
                    </div>
                    @if (Model.Para3Subheading == null)
                    {
                        <br />
                    }
                }
                @if (Model.Para3Subheading != null)
                {
                    <div class="row justify-content-center align-items-center">
                        <div class="font-style-article-subhead thumb-text-block d-none d-sm-none d-md-none d-lg-block d-xl-block" id="_ArticlePara3Subheading">@Model.Para3Subheading</div>
                        <div class="font-style-article-subhead-medium thumb-text-block d-none d-sm-block d-md-block d-lg-none d-xl-none" id="_ArticlePara3Subheading">@Model.Para3Subheading</div>
                        <div class="font-style-article-subhead-small thumb-text-block d-block d-sm-none d-md-none d-lg-none d-xl-none" id="_ArticlePara3Subheading">@Model.Para3Subheading</div>
                    </div>
                }
                @if (Model.Para3 != null)
                {
                    <div class="row">
                        <div class="font-style-content-black d-none d-sm-none d-md-none d-lg-block d-xl-block" id="_ArticlePara3">@Model.Para3</div>
                        <div class="font-style-content-medium-black d-none d-sm-block d-md-block d-lg-none d-xl-none" id="_ArticlePara3">@Model.Para3</div>
                        <div class="font-style-content-small-black d-block d-sm-none d-md-none d-lg-none d-xl-none" id="_ArticlePara3">@Model.Para3</div>
                    </div>
                    <br />
                }
                else if (Model.Image3 != null && Model.Para3 == null)
                {
                    <div class="row justify-content-center align-items-center">
                        @if (Model.ArticleId.Substring(Model.ArticleId.LastIndexOf("/") + 1) != "Preview")
                        {
                            <img class="img-fluid max-width: 120%; height: auto;" src="@Html.DisplayFor(modelItem => Model.Image3)" id="_ArticleImage3" width="480" height="260" style="margin:0px 20px;" alt="Article Environment Image 3">
                        }
                        else
                        {
                            <img class="img-fluid max-width: 120%; height: auto;" src="~/previewimages/@Model.Image3" id="_ArticleImage3" width="480" height="260" style="margin:0px 20px;" alt="Article Title Image">
                        }
                    </div>
                    @if (Model.Image3Caption == null && Model.Para4Subheading == null && Model.Para4 == null)
                    {
                        <br />
                    }
                }
                else if (Model.Video3 != null && Model.Para3 == null && Model.Image3 == null)
                {
                    <div class="row justify-content-center align-items-center">
                        <div class="video-container">
                            <iframe src="@Html.DisplayFor(modelItem => Model.Video3)" id="_ArticleVideo3" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen onclick="gtag('event','click',{'event_category':'Click','event_label':'UserArticleVideo3-Embedded-Video'});"></iframe>
                        </div>
                    </div>
                }
                @if (Model.Image3Caption != null)
                {
                    <div class="row justify-content-center align-items-center">
                        <div class="font-small thumb-text-block" id="_ArticleImage3Caption">@Model.Image3Caption</div>
                    </div>
                    @if (Model.Para4Subheading == null)
                    {
                        <br />
